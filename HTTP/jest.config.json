{
  "preset": "ts-jest",
  "transform": {
    "\\.tsx?$": "ts-jest",
    "\\.jsx?$": "babel-jest"
  },
  "testEnvironment": "jsdom",
  "setupFilesAfterEnv": [
    "./test/_meta/setupGlobal.js"
  ],
  "globals": {
    "ts-jest": {
      "diagnostics": {
        "ignoreCodes": [
          151001
        ]
      }
    }
  },
  "collectCoverage": true,
  "coverageDirectory": "./test/_coverage/"
}
