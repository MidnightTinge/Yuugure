    "target": "ES2020",
    "module": "ES6",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "outDir": "dist",
    "jsx": "react"
